<div class="container" id="map-page">
  <div class="col-xs-12 col-md-6 map-view">
    <img class="col-xs-12 thumbnail" id="mv-map" src="http://placehold.it/500x500">
    <div class="col-xs-12" id="mv-info">
      <div class="col-xs-12" id="mv-title-row">
        <h1 class="col-xs-10" id="mv-title">Title here</h1>
        <div class="col-xs-2 likes">
          <span class="glyphicon glyphicon-star" id="liked-star" aria-hidden="true"></span>
          <h3 id="number-of-likes">#</h3>
        </div>
        <div class="col-xs-12" id="mv-description-row">
            <h5 id="mv-description">I am a map!</h5>
        </div>
        <div class="col-xs-12" id="mv-creator-row">
          <h5 id="mv-creator-name">Creator</h5>
        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-md-6 places-view">
    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

    </div>
  </div>
</div>